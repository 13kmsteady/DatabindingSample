<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.i3kmsteady.databindingsample">
	
	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ui.BasicActivity" />
		<activity android:name=".ui.ExpressionsActivity" />
		<activity android:name=".ui.EventActivity" />
		<activity android:name=".ui.ObservableActivity" />
		<activity android:name=".ui.BindingAdaptersActivity" />
		<activity android:name=".ui.SpinnerActivity" />
	</application>

</manifest>